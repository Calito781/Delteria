CREATE TABLE CARD (ACCOUNT VARCHAR(255) NOT NULL, BODY VARCHAR(255), COLORS BLOB(2147483647), EXPIRY_DATE TIMESTAMP, HAT VARCHAR(255), HEAD VARCHAR(255), ISONLINE SMALLINT DEFAULT 0, NICKNAME VARCHAR(255), ONLINEDURATION BIGINT, PRIMARY KEY (ACCOUNT))
CREATE TABLE FILE (FILENAME VARCHAR(255) NOT NULL, CONTENT BLOB(2147483647), PRIMARY KEY (FILENAME))
CREATE TABLE ITEM (ITEM_ID VARCHAR(255) NOT NULL, DESCRIPTION VARCHAR(255), ICON_FILE VARCHAR(255), NAME VARCHAR(255), PROPERTIES BLOB(2147483647), QUEST_ITEM SMALLINT DEFAULT 0, PRIMARY KEY (ITEM_ID))
